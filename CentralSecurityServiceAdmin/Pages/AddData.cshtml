@page
@model CentralSecurityServiceAdmin.Pages.AddDataModel
@{
    ViewData["Title"] = "Add Data";
}

@section MetaTags {
    <meta name="keywords" content="Central Security Service, CIA, NSA, MI6, SIS, MI5, GCHQ, Medical, Freemasons, In Memory, Heritage, Legacy, Societal Development">
}

<h1>Add Data</h1>

<form method="POST">
    <div>Message: <span>@Model.Message</span></div>
    <br />
    <div>
        <input type="checkbox" asp-for="OnlyAddIfUnique" checked="checked" />
        <label asp-for="OnlyAddIfUnique">Only Add If Unique</label>
    </div>
    <br />
    <input type="submit" name="Action" value="Add Data" />
</form>
